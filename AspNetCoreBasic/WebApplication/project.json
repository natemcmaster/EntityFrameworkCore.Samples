{
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "debugType": "portable"
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Design": {
      "type": "build",
      "version": "1.0.0-preview2-*"
    },
    "DataLibrary": {
      "target": "project"
    },
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0-rc3-*"
    },
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc3-*",
    "Microsoft.AspNetCore.Hosting": "1.0.0-rc3-*",
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc3-*",
    "Microsoft.AspnetCore.Diagnostics": "1.0.0-rc3-*",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc3-*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0-rc3-*",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc3-*"
  },
  "frameworks": {
    "netcoreapp1.0": {}
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "appsettings.json"
    ]
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
  "tools": {
    "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-*"
  }
}